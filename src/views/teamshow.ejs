<%- include("_header") -%>

<h1>Detail record</h1>

<form action="/teamshow/<%= model.id %>" method="post">
  <div class="form-horizontal">

      
    <div class="form-group row">
        <label class="col-form-label col-sm-2" for="id">Id: <%= model.id %></label>
    </div>      
    <div class="form-group row">
        <label class="col-form-label col-sm-2" for="name">Name: <%= model.name %></label>
    </div>
      

    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <a class="btn btn-outline-dark cancel" href="/teams">Back</a>
      </div>
    </div>
  </div>
</form>

<%- include("_footer") -%>